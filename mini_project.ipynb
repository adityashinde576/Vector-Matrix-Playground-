    "import numpy as np\n",
    "\n",
    "X = np.array([[1, 2], [3, 4], [5, 6]])\n",
    "W = np.array([[0.2], [0.8]])\n",
    "\n",
    "y = np.dot(X, W)\n",
    "\n",
    "y_norm = (y - np.mean(y)) / np.std(y)\n",
    "\n",
    "print(\"Predictions:\\n\", y_norm)\n"
